{
  "expo": {
    "name": "LogYUsers",
    "slug": "LogYUsers",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/usr.png",
    "plugins": [
      "expo-image-picker"
    ],
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/usr.png",
      "resizeMode": "contain",
      "backgroundColor": "#34054F"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/usr.png",
        "backgroundColor": "#34054F"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.davit036.LogYUsers"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
